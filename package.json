    "express"
  ],
  "author": "Daniel Pedroso <daniel.exe@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.4.0",
    "co": "^4.6.0",
    "is-generator-function": "^1.0.3"
  },
  "devDependencies": {
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "should": "^8.3.2"
  }
}

{
  "name": "express_cowrap",
  "version": "1.0.2",
  "description": "co-wrap function to use proper error handling and easier asynchronous operations (by using generators) with Express",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --recursive --reporter spec && npm run coverage",
